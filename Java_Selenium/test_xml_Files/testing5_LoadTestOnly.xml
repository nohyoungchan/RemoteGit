  <test name="VoiceACD Scenarios"> <!--### This is VoiceACD scenarios ### -->
	  
      <classes>
	  <class name="HBTestesNG.TestCases.Test_Initiate">
	  </class>
	  
	   <class name="HBTestesNG.TestCases.Test_Load">
        <methods>  
          <include name="Load" /> <!--### This applies for all load scenarios ### -->
        </methods>
      </class>
      
   
       
    </classes>
  </test>
</suite> 
